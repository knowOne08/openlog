{
  "name": "minio-microservice",
  "version": "1.0.0",
  "description": "Independent MinIO microservice for file upload and retrieval",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
